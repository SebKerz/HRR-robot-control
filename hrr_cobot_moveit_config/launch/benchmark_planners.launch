<?xml version="1.0"?>
<launch>
  <!-- benchmark options file -->
  <arg name="package_name" default="racer5-0-80_moveit_config" doc="moveit package to be benchmarked"/>
  <arg name="config_name" default="benchmark_all" doc="moveit package to be benchmarked"/>

  <arg name="bench_opts" default="$(find $(arg package_name))/config/$(all config_name).yaml"/>

  <!-- Load robot settings -->
  <include file="$(find $(arg package_name))/launch/settings/planning_context.launch.xml">
    <arg name="load_robot_description" value="true"/>
  </include>
  
  <!-- Load all planning pipelines that will be benchmarked -->
  <include ns="moveit_run_benchmark" file="$(find racer5-0-80_moveit_config)/launch/settings/ompl.launch.xml"/>
  <include ns="moveit_run_benchmark" file="$(find racer5-0-80_moveit_config)/launch/settings/stomp.launch.xml"/>
  <include ns="moveit_run_benchmark" file="$(find racer5-0-80_moveit_config)/launch/settings/chomp.launch.xml"/>

  <!-- Load warehouse containing scenes and queries to benchmark -->
  <include file="$(find racer5-0-80_moveit_config)/launch/warehouse_db.launch" />

  <!-- Launch benchmark node -->
  <node name="moveit_run_benchmark" pkg="moveit_ros_benchmarks" type="moveit_run_benchmark" output="screen" required="true">
    <rosparam command="load" file="$(arg bench_opts)"/>
  </node>

  <!-- Start Benchmark Executable 
  <node name="$(anon moveit_benchmark)" pkg="moveit_ros_benchmarks" type="moveit_run_benchmark" 
  	args="$(arg cfg) -benchmark-planners" respawn="false" output="screen">
    <rosparam command="load" file="$(find racer5-0-80_moveit_config/config/kinematics.yaml"/>
    <rosparam command="load" file="$(find racer5-0-80_moveit_config/config/ompl_planning.yaml"/>
  </node>
-->
</launch>
